<div class="ms-tabsSetting-content">
  <% if @user.errors.any? %>
    <div class="row">
      <div class="col s12">
        <div id="error_explanation" class="ms-alert ms-alert_error">
          <!-- <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2> -->
          <ul>
          <% @user.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      </div>
    </div>
  <% end %>
    <!-- <%= hidden_field_tag :authenticity_token, form_authenticity_token %> -->
  <div id="profile">
    <%= render 'users/personal_info'%>
  </div>
  <div id="security" class="ms-tabsSetting-content">
    <%= render 'users/security'%>
  </div>
  <div id="invitations" class="ms-tabsSetting-content">
  </div>
</div>
