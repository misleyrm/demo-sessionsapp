$(document).on('turbolinks:load',function(){
  if(!$('#ms-tabsSetting').length){
      $('header .nav-wrapper').append('<div id="ms-tabsSetting"></div>');
  }
  $('#ms-tabsSetting').html('<%= j render partial: 'users/tabsSetting' %>');
  $('body').addClass('expand-header');
  $('nav a.button-collapse').css('display','block');
  $('.navigationbar div#slide-out').css({transform:'translateX(-100%)'});
  $('#page-content').html('<%= j render 'users/edit' %>');
  $('ul.tabs').tabs();
})
