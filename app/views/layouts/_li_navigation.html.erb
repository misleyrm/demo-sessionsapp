<li class="bold <%= (active) ? 'active' : '' %>" data-name="<%=list.name %>" data-nav-list-id="<%=list.id %>" id= "drop_zone_<%=list.id %>" >
  <a id="<%= list.id %>" href="#" data-href= "<%=list_path(list) %>" data-remote="true">
       <%= yield %>
      <div class="right">
        <% if (list.collaborations?) && (list.collaboration_users.count!=0) %>
            <i class="material-icons bar-collaboration-icon">group</i>
        <% end %>
        <span class="bar-number-task"><%=user.num_incompleted_tasks(list)%></span>
     </div>
  </a>
</li>
