$(document).on('turbolinks:load',function(){
    var $form =  $('#application-date form');
    var $content = $('#page-content');
    if($('#new-list').length){
      $('#new-list').remove();
    }
    if($('#ms-user-edit').length){
      $('#ms-user-edit').remove();
    }
    if($('#ms-tabsSetting').length){
        $('#ms-tabsSetting').remove();
    }
    if ($('body').hasClass('expand-header')){
        $('body').removeClass('expand-header');
    }
    console.log("<%= @_current_list.id %>")

    $content.html("<%= j render partial: 'lists/list', locals: { list: current_list, user: current_user } %>");
    // $('#collaboration_users').html("<%#= j render partial: 'list_users', locals: {list: current_list} %>");
    // $('#show_collaboration_users').html("");

    $("#ms-link-listSetting").html("<%= j render partial: 'lists/link_listSetting', locals:{list: @list} %>");

    // $('#list').html("<%#= j render partial: 'users/list_user', locals:{user: current_user, list: @list} %>");
    $form.attr("action","lists/<%= @list.id %>");
    $form.attr("id","edit_list_<%= @list.id %>");
    $('.edit_task').submitOnCheck();


    // $('#detail').triggeredAutocomplete({
    //   hidden: '#hidden_inputbox',
    //   source: "/lists/28/complete_users",
    //   trigger: "@"
    // });

    $('#nav-list').html("<%= j render partial: 'lists/nav_list_name', locals: {list: @list} %>");
    deadlineDatepicker();
});
