    border-left-color: #bee5eb;
}

.close span {
    font-size: 1.8rem;
}

.alert-dismissible .close {
    position: absolute;
    top: 0;
    right: 0;
    padding: .6rem .6rem .6rem 1rem;
    color: inherit;
}

.close {
    float: right;
    font-size: 1.5rem;
