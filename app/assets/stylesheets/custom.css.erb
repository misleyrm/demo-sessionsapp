.show_blockers .blocker ul.action-menu {
    display: inline;
    position: absolute;
    right: 0;
    top: 0;
    width: 50px;
    line-height: 20px;
    min-height: 16px;
}

.permitted .show_blockers .blocker:hover{
  background-color: rgba(185, 185, 185, 0.07);
}

.show_blockers div.blocker:not(.active) ul.action-menu {
  opacity: 0;
}

.show_blockers div.blocker.active ul.action-menu {
  -webkit-transition: opacity .05s linear,visibility .05s linear;
  -moz-transition: opacity .05s linear,visibility .05s linear;
  transition: opacity .05s linear,visibility .05s linear;

}
.show_blockers ul.action-menu li {
    display: inline-table;
    float: left
    padding: 0 5px;
  }

.show_blockers ul.action-menu li i,
.show_blockers a.blocker_detail{
  line-height: 20px;
}

.show_blockers .blocker{
  position: relative;
  height: 20px;
}
.no-permitted #my-add-task{
  display: none;
}

.no-permitted ul.menu-option > li a,
.no-permitted ul.menu-option > li form,
.no-permitted ul.menu-option > li div.input-group{
  pointer-events: none;
  cursor: not-allowed;
  display: none;
}
.no-permitted ul.menu-option > li a{
  pointer-events: none;
}

.permitted ul.menu-option > li i[data-has-blockers],
.no-permitted ul.menu-option > li i[data-has-blockers='false']{
  display: none;
}

