/* STYLE FOR CARD ELEMENT*/
.card {
  margin: 0 auto!important;
  padding: 1.5rem 0.6rem 1.5rem 0.8rem;
  border-radius: 2px!important;
  -moz-border-radius: 2px;
  /* for Safari y Chrome*/
  -webkit-border-radius: 2px;

}
.card-title{
  margin: 0!important;
}
div .card{
  box-shadow: 0px 0px 100px rgba(147, 161, 185, 0.59);
  -moz-box-shadow: 0px 0px 100px rgba(147, 161, 185, 0.59);
  -webkit-box-shadow: 0px 0px 100px rgba(147, 161, 185, 0.59);
 color: #848994;
}
.card .card-title {
  border-bottom: 1px solid rgba(160,160,160,0.2);
  min-height: 122px;
}
.card .card-action {
    border-top: none;
    padding: 0px;
}
#ms-user-new .card .card-title{
  min-height: 122px;
}

.card .card-title h1{
  margin-top: 10px;
  margin-bottom: 0px;
  font-size: 2rem!important;
  text-transform: uppercase;
  color: #55C2B9!important;
  line-height: 1.5rem;
}
.container .card .card-action a:not(.btn):not(.btn-large):not(.btn-floating) {
  color: #989797;
}
.container .card .card-action a:not(.btn):not(.btn-large):not(.btn-floating):hover {
  color: #55C2B9;
}

.container{
  padding-top: 20px;
}

.card .card-content a[class="btn"],
.card .card-content a[class="btn"] input{
  width: 100%;
  height: 54px;
}
/* END STYLE FOR CARD ELEMENT */
